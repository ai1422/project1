/**index.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
  box-sizing: border-box;
} 
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 90rpx;
  height: 90rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.cell{
  background-color:#fff;
  font-size:16px;  
}
.cell-item{
  padding:20rpx 30rpx;
  border-bottom:1rpx solid #ddd;
}

/*四个图标的样式*/
.navs{
  display:flex;
}
.nav-item{
  width:25%;
  display:flex;
  align-items: center;
  flex-direction: column;
  padding:20rpx;
}
.nav-item .nav-image{
  width:60rpx;
  height:60rpx;
  margin:5rpx;
}
.nav-item text{
  margin-top:20rpx;
  font-size:25rpx;
  color:gray;
}
